<%= simple_form_for(@pet) do |f| %>
  <%= f.input :name %>
  <%= f.input :age %>
  <%= f.input :race%>
  <%= f.input :address%>
  <%= f.input :species %>
  <%= f.input :description%>
  <%= f.input :photo_url%>
  <%= f.input :habitat , collection: ["🌴 J", "🌳 F", "🏜️ D", "🌊 O", "🦒 S"]   %>
  <%= f.submit %>
<% end %>
